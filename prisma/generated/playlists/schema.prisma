generator client {
  provider = "prisma-client-js"
  output   = "../prisma/generated/playlists"
}

datasource db {
  provider = "sqlite"
  url      = "file:../prisma/database/playlist.db"
}

model playlists {
  id              Int     @id @default(autoincrement())
  playlistId      String?
  channelId       String?
  title           String?
  description     String?
  image           String?
  channelTitle    String?
  defaultLanguage String?
  itemCount       Int?
  imageUrl        String?
  category_1      String?
  category_2      String?
  category_3      String?
  category_4      String?
  category_5      String?
  category_6      String?
  category_7      String?
  category_8      String?
  category_9      String?
  category_10     String?
}
