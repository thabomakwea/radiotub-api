generator client {
  provider = "prisma-client-js"
  output   = "../prisma/generated/playlists"
}

datasource db {
  provider = "sqlite"
  url      = "file:../prisma/database/playlist.db"
}

model playlists {
  id              Int     @id @default(autoincrement())
  playlistId      String?
  channelId       String?
  title           String?
  description     String?
  image           String?
  channelTitle    String?
  defaultLanguage String?
  itemCount       Int?
}
